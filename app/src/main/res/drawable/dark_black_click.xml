<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#ff999999" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff5d5d5d" android:endColor="#ff5d5d5d" android:angle="270.0" />
            <solid android:color="#ff5d5d5d" />
        </shape>
    </item>
</selector>